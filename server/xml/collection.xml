<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plugin id="collection" version="1.0">
  <action id="executecollection">
	  <string id="collectionid" model="collections" model_key="collectionid" model_show="name" />
  </action>
  <action id="stopcollection">
	  <string id="collectionid" model="collections" model_key="collectionid" model_show="name" />
  </action>
  
  <execute id="clone">
	  <string id="collectionid" model="collections" model_key="collectionid" model_show="name" />
  </execute>
  
  <collection>
	  <string id="name" />
	  <stringlist id="actions" />
	  <stringlist id="conditions" />
	  <stringlist id="events" />
	  <stringlist id="categories" />
	  <bool id="enabled" />
  </collection>
  
  <section>collections</section>
</plugin>
