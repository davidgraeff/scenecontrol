<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plugin>
	<titel>RoomControl Leds (udp)</titel>
	<description>Kontrolliert die Leuchtdioden des RoomControl Systems</description>
	<sections>
		<switch>
			<titel>Leuchtdioden</titel>
			<description>Kontrolliert die Leuchtdioden und Rollos des RoomControl Systems</description>
		</switch>
	</sections>
	<udpled.value_relative>
		<titel>LED Wert Relativ</titel>
		<description>Setzt LED Wert</description>
		<channel><titel>Leuchtkanal</titel></channel>
		<value><titel>Wert</titel></value>
		<fade><titel>Farbübernahme</titel>
		<entry>Sofort</entry><entry>Überblenden</entry><entry>Überblenden (Flashy)</entry>
		</fade>
	</udpled.value_relative>
	<udpled.value_absolut>
		<titel>LED Wert Absolut</titel>
		<description>Setzt LED Wert</description>
		<channel><titel>Leuchtkanal</titel></channel>
		<value><titel>Wert</titel></value>
		<fade><titel>Farbübernahme</titel>
		<entry>Sofort</entry><entry>Überblenden</entry><entry>Überblenden (Flashy)</entry>
		</fade>
	</udpled.value_absolut>
	<udpled.value_invers>
		<titel>LED Wert Invertieren</titel>
		<description>Setzt LED Wert</description>
		<channel><titel>Leuchtkanal</titel></channel>
		<fade><titel>Farbübernahme</titel>
		<entry>Sofort</entry><entry>Überblenden</entry><entry>Überblenden (Flashy)</entry>
		</fade>
	</udpled.value_invers>
	<udpled.moodlight>
		<titel>LED Moodlight</titel>
		<description>Setzt LED Wert</description>
		<channel><titel>Leuchtkanal</titel></channel>
		<moodlight><titel>Moodlight</titel></moodlight>
	</udpled.moodlight>
	<udpled.value>
		<titel>LED Wert Änderung</titel>
		<description>LED Wert hat sich verändert</description>
		<channel><titel>Leuchtkanal</titel></channel>
		<value><titel>Wert</titel></value>
	</udpled.value>
	<udpled.condition>
		<titel>LED Wert Bedingung</titel>
		<description>Led Wert muss zwischen Unter und Obergrenze liegen</description>
		<channel><titel>Leuchtkanal</titel></channel>
		<lower><titel>Untere Grenze</titel></lower>
		<upper><titel>Obere Grenze</titel></upper>
	</udpled.condition>
	<udpled.name>
		<titel>LED Namen</titel>
		<description>LED Namen</description>
		<channel><titel>Leuchtkanal</titel></channel>
		<name><titel>Name</titel></name>
	</udpled.name>
</plugin>