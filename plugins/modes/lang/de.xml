<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<plugin>
     <titel>Modes</titel>
     <description>Ermöglicht das Speichern von Zuständen und das Reagieren auf Veränderungen des Zustandes</description>
  <changemode>
      <titel>Modus ändern</titel>
      <description>Ändert den Modus</description>
      <mode><titel>Modus</titel></mode>
  </changemode>
  <modeevent>
      <titel>Modusereignis</titel>
      <description>Modus hat sich geändert</description>
      <mode><titel>Modus</titel></mode>
  </modeevent>
  <modecondition>
      <titel>Modusbedingung</titel>
      <description>Erfüllt wenn im angegeben Modus</description>
      <mode><titel>Modus</titel></mode>
  </modecondition>
  <modechanged>
      <titel>Modusereignis</titel>
      <description>Modus hat sich geändert</description>
      <mode><titel>Modus</titel></mode>
  </modechanged> 
  <serverstate>
      <titel>Serverstatus</titel>
      <description>Serverstatus hat sich geändert</description>
      <state><titel>Serverstatus</titel><entry>Bereit</entry><entry>Abschalten</entry></state>
  </serverstate>
</plugin>